{
  "name": "soccer-prediction-frontend",
  "version": "1.0.0",
  "description": "Frontend React application for soccer prediction",
  "private": true,
  "dependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "typescript": "^5.2.2",
    "web-vitals": "^3.5.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run resolve && react-scripts build",
    "build:force": "npm run resolve && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "resolve": "npx npm-force-resolutions install",
    "clean-install": "npm run resolve && rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "clean-build": "npm run clean-install && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "react-scripts": "5.0.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "npm-force-resolutions": "^0.0.10"
  },
  "resolutions": {
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "ajv-keywords": "5.1.0",
    "webpack": "5.88.2",
